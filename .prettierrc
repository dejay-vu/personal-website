{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "printWidth": 80,
  "tabWidth": 2,
  "trailingComma": "all",
  "singleQuote": true,
  "semi": true,
  "importOrder": [
    "^react",
    "^next",
    "^next/(.*)$",
    "<THIRD_PARTY_MODULES>",
    "^@/styles/(.*)$",
    "^@/lib/(.*)$",
    "^@/utils/(.*)$",
    "^@/services/(.*)$",
    "^@/components/(.*)$",
    "^[./]",
    "^[../]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
